{
  "name": "etherscan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.2.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src$1"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ],
    "plugins": [
      "eslint-plugin-import-helpers",
      "@typescript-eslint"
    ],
    "rules": {
      "no-undef": "off",
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-tag-spacing": [
        2,
        {
          "closingSlash": "never",
          "beforeSelfClosing": "always",
          "afterOpening": "never",
          "beforeClosing": "never"
        }
      ],
      "no-unused-vars": "off",
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "@typescript-eslint/no-empty-interface": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "max-len": [
        "error",
        120,
        2,
        {
          "ignoreComments": true
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "semi": [
        "error",
        "always"
      ],
      "sort-imports": [
        "error",
        {
          "ignoreCase": true,
          "ignoreDeclarationSort": true
        }
      ],
      "import-helpers/order-imports": [
        "error",
        {
          "groups": [
            "/^react$/",
            "module",
            "/^~/",
            "parent",
            [
              "sibling",
              "index"
            ]
          ],
          "newlinesBetween": "always",
          "alphabetize": {
            "order": "asc",
            "ignoreCase": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "circular-dependency-plugin": "^5.2.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.4"
  }
}
